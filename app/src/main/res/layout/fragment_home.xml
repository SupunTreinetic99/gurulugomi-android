<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context="com.treinetic.whiteshark.fragments.home.HomeFragment">


    <com.treinetic.whiteshark.customview.CustomRecyclerView
            android:id="@+id/customListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" />


    <com.treinetic.whiteshark.customview.BottomLoginSheet
            android:id="@+id/bottomSheetDialogCV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:layout_gravity="bottom"/>

</FrameLayout>

